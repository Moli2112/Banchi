<Window x:Class="Banchi.ImportExportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Banchi"
        mc:Ignorable="d"
        Title="Importazione ed esportazione dati" 
        Height="300" Width="1000"
        Style="{StaticResource StileFinestre}">
    <StackPanel>
        <TextBlock Text="Importazione ed esportazione dati" 
                   HorizontalAlignment="Center" VerticalAlignment="Top" 
                   FontSize="50" Margin="0,10,0,0"/>
    <TextBlock Text="Ancora da fare!" 
                   HorizontalAlignment="Center" VerticalAlignment="Top" 
                   FontSize="80" Margin="0,10,0,0"/>
    </StackPanel>
</Window>
