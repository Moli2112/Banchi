<Window x:Class="Banchi.ConfigurazioniWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Banchi"
        mc:Ignorable="d"
        Title="Configurazione del programma" Height="300" Width="900"
                Style="{StaticResource StileFinestre}">
    <StackPanel>
        <TextBlock Text="Configurazione del programma" 
                   HorizontalAlignment="Center" VerticalAlignment="Top" 
                   FontSize="50" Margin="0,10,0,0"/>
        <TextBlock Text="Ancora da fare!" 
                   HorizontalAlignment="Center" VerticalAlignment="Top" 
                   FontSize="80" Margin="0,10,0,0"/>
    </StackPanel>
</Window>
